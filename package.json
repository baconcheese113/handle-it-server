{
  "name": "server",
  "version": "1.0.0",
  "description": "Server code for handleit",
  "main": "index.ts",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "start": "node dist",
    "dev": "env-cmd ts-node-dev --transpile-only --no-notify index.ts",
    "push-db": "npm run push-db:dev && npm run push-db:prod",
    "push-db:prod": "env-cmd -f ./.env.production prisma db push",
    "push-db:dev": "env-cmd -f ./.env prisma db push",
    "migrate-reset-prod": "env-cmd -f ./.env.production prisma migrate reset",
    "build": "npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "prisma:format": "prisma format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "env-cmd": "^10.1.0",
    "prisma": "^3.11.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "apollo-server": "^3.6.6",
    "apollo-server-errors": "^3.3.1",
    "bcrypt": "^5.0.1",
    "firebase-admin": "^10.0.2",
    "graphql": "^16.3.0",
    "graphql-iso-date": "^3.6.1",
    "jsonwebtoken": "^8.5.1",
    "nexus": "^1.3.0",
    "nexus-plugin-prisma": "^0.35.0"
  }
}
