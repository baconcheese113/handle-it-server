{
  "name": "server",
  "version": "1.0.0",
  "description": "Server code for handleit",
  "main": "index.ts",
  "engines": {
    "node": "18.7.0",
    "npm": "8.15.0"
  },
  "scripts": {
    "start": "node dist",
    "dev": "env-cmd ts-node-dev --transpile-only --no-notify index.ts",
    "lint": "eslint --ignore-path .prettierignore --ext .js,.ts .",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.+(js|ts|json)\"",
    "test": "env-cmd -f ./.env.testing ts-node-dev --transpile-only --no-notify index.ts",
    "push-db": "npm run push-db:test && npm run push-db:dev && npm run push-db:prod",
    "push-db:prod": "env-cmd -f ./.env.production prisma db push",
    "push-db:test": "env-cmd -f ./.env.testing prisma db push",
    "push-db:dev": "env-cmd -f ./.env prisma db push",
    "pull-db:dev": "env-cmd -f ./.env prisma db pull --force",
    "seed": "ts-node prisma/seed.ts",
    "migrate-reset-prod": "env-cmd -f ./.env.production prisma migrate reset",
    "build": "npm -s run generate && tsc",
    "generate": "prisma generate",
    "prisma:format": "prisma format"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^16.11.41",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.21.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "prisma": "^3.15.2",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@pothos/core": "^3.13.0",
    "@pothos/plugin-prisma": "^3.21.2",
    "@pothos/plugin-scope-auth": "^3.11.2",
    "@prisma/client": "^3.15.2",
    "apollo-server": "^3.9.0",
    "apollo-server-errors": "^3.3.1",
    "bcrypt": "^5.0.1",
    "firebase-admin": "^11.0.0",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "jsonwebtoken": "^8.5.1"
  }
}